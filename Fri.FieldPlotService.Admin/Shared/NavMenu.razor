@inject IStringLocalizer<Resources.Strings> localizer

<AuthorizeView>
    <Authorized>
        <MudNavMenu>
            <MudNavLink Href="/" Match="NavLinkMatch.All">@localizer["Menu_Home"]</MudNavLink>
            <MudNavLink Href="/vendors" Match="NavLinkMatch.All">@localizer["Menu_Vendors"]</MudNavLink>
            <MudNavLink Href="/users" Match="NavLinkMatch.Prefix">@localizer["Menu_Users"]</MudNavLink>
            <MudNavLink Href="/projects" Match="NavLinkMatch.Prefix">@localizer["Menu_Projects"]</MudNavLink>
            <MudNavLink Href="/plots" Match="NavLinkMatch.Prefix">@localizer["Menu_Plots"]</MudNavLink>
            <MudNavLink Href="/export" Match="NavLinkMatch.Prefix">@localizer["Menu_Export"]</MudNavLink>
        </MudNavMenu>
    </Authorized>
    <NotAuthorized>
        <MudNavMenu>
            <MudNavLink Href="/" Match="NavLinkMatch.All">@localizer["Menu_Home"]</MudNavLink>
            <MudNavLink Href="/authentication/login" Match="NavLinkMatch.Prefix">@localizer["General_Login"]</MudNavLink>
        </MudNavMenu>
    </NotAuthorized>
</AuthorizeView>

